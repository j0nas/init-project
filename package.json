{
  "name": "create-new-project",
  "version": "0.1.0",
  "scripts": {
    "build": "tsc --project .",
    "build:watch": "tsc --project . --watch"
  },
  "description": "Default Next.js project scaffolding",
  "keywords": [
    "scaffold",
    "cli"
  ],
  "author": {
    "name": "Jonas Jensen",
    "email": "jonas.jensen@msn.com",
    "url": "https://jonas-jensen.com"
  },
  "license": "MIT",
  "engines": {
    "node": ">=8"
  },
  "bin": {
    "init": "dist/index.js"
  },
  "files": [
    "resources",
    "src",
    "dist"
  ],
  "dependencies": {
    "cpy": "^7.0.1",
    "execa": "^1.0.0",
    "meow": "^5.0.0",
    "npm-check-updates": "^2.15.0",
    "opn": "^5.4.0",
    "valid-filename": "^2.0.1"
  },
  "devDependencies": {
    "@types/cpy": "^5.1.0",
    "@types/execa": "^0.9.0",
    "@types/meow": "^5.0.0",
    "@types/node": "^10.12.19",
    "@types/opn": "^5.1.0",
    "husky": "^1.3.1",
    "prettier": "^1.16.3",
    "pretty-quick": "^1.10.0",
    "typescript": "^3.3.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  }
}
